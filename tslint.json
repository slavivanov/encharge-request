{
  "extends": [
    "tslint-config-airbnb",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rules": {
    "import-name": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ],
    "object-shorthand-properties-first": false
  }
}
